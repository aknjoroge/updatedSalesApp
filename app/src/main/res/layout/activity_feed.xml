<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"

    android:background="@color/white"
    android:layout_height="match_parent"
    tools:context=".feed">
    <TextView
        android:layout_alignParentTop="true"
        android:id="@+id/textView26"
        android:layout_width="match_parent"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:background="@color/white"
        android:fontFamily="@font/fontone"
        android:textSize="16dp"
        android:layout_marginTop="1dp"
        android:padding="10dp"
        android:layout_marginBottom="4dp"
        android:layout_height="wrap_content"
        android:text="@string/socialfeed" />


    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/feedrecysler"
        android:layout_width="match_parent"
        android:layout_below="@id/textView26"
      android:layout_above="@id/bottomnavnew"
        android:layout_height="match_parent"

        android:background="@color/white"
        android:scrollbars="vertical">

    </androidx.recyclerview.widget.RecyclerView>
    <com.etebarian.meowbottomnavigation.MeowBottomNavigation
        android:id="@+id/bottomnavnew"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        app:mbn_rippleColor="#2f424242"

        android:layout_alignParentBottom="true"
        app:mbn_backgroundBottomColor="@color/blue"
        android:layout_marginBottom="0.2dp"
        app:mbn_circleColor="@color/white"
        app:mbn_countBackgroundColor="#ff6f00"
        app:mbn_countTextColor="@color/white"
        app:mbn_defaultIconColor="@color/white"
        app:mbn_selectedIconColor="#3c415e"
        tools:layout_editor_absoluteY="675dp">


    </com.etebarian.meowbottomnavigation.MeowBottomNavigation>


</RelativeLayout>